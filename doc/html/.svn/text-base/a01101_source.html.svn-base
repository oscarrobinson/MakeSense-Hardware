<a name="l00657"></a>00657 <span class="keyword">static</span> <span class="keywordtype">int</span> add_to_rxbuf(uint8_t * src)
<a name="l00658"></a>00658 {
<a name="l00659"></a>00659     <span class="keywordflow">if</span>(RXBUFS_FULL()){
<a name="l00660"></a>00660         <span class="keywordflow">return</span> 0;
<a name="l00661"></a>00661     } 
<a name="l00662"></a>00662     
<a name="l00663"></a>00663     memcpy(stm32w_rxbufs[last], src, src[0] + 1);
<a name="l00664"></a>00664 <span class="preprocessor">#if RADIO_RXBUFS &gt; 1</span>
<a name="l00665"></a>00665 <span class="preprocessor"></span>    last = (last + 1) % RADIO_RXBUFS; 
<a name="l00666"></a>00666     <span class="keywordflow">if</span>(first == -1){
<a name="l00667"></a>00667         first = 0;
<a name="l00668"></a>00668     }
<a name="l00669"></a>00669 <span class="preprocessor">#endif</span>
