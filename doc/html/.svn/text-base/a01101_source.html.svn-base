<a name="l00607"></a>00607   PRINTF(<span class="stringliteral">&quot;stm32w_radio_process: started\r\n&quot;</span>);
<a name="l00608"></a>00608   
<a name="l00609"></a>00609   <span class="keywordflow">while</span>(1) {
<a name="l00610"></a>00610     
<a name="l00611"></a>00611     <a class="code" href="a01866.html#gae5edd3d10ed29b7a6ed4a42e0690738b" title="Yield the currently running process until a condition occurs.">PROCESS_YIELD_UNTIL</a>(ev == PROCESS_EVENT_POLL);    
