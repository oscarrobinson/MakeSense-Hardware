<a name="l00615"></a>00615 <span class="preprocessor">#if DEBUG &gt; 1</span>
<a name="l00616"></a>00616 <span class="preprocessor"></span>    <span class="keywordflow">for</span>(uint8_t c=1; c &lt;= RCVD_PACKET_LEN; c++){
<a name="l00617"></a>00617       PRINTF(<span class="stringliteral">&quot;%x&quot;</span>,stm32w_rxbuf[c]);
<a name="l00618"></a>00618     }
<a name="l00619"></a>00619     PRINTF(<span class="stringliteral">&quot;\r\n&quot;</span>);
<a name="l00620"></a>00620 <span class="preprocessor">#endif</span>
<a name="l00621"></a>00621 <span class="preprocessor"></span>    
<a name="l00622"></a>00622     <a class="code" href="a01801.html#gaa540c9abf0db13d27881b616f7794824" title="Clear and reset the packetbuf.">packetbuf_clear</a>();
