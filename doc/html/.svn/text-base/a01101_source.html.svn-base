<a name="l00455"></a>00455 <span class="keyword">static</span> <span class="keywordtype">int</span> stm32w_radio_off(<span class="keywordtype">void</span>)
<a name="l00456"></a>00456 {  
<a name="l00457"></a>00457   <span class="comment">/* Any transmit or receive packets in progress are aborted.</span>
<a name="l00458"></a>00458 <span class="comment">   * Waiting for end of transmission or reception have to be done.</span>
<a name="l00459"></a>00459 <span class="comment">   */</span>
<a name="l00460"></a>00460   <span class="keywordflow">if</span>(locked)
<a name="l00461"></a>00461   {
<a name="l00462"></a>00462     PRINTF(<span class="stringliteral">&quot;stm32w: try to off while sending/receiving (lock=%u).\r\n&quot;</span>, locked);
<a name="l00463"></a>00463     <span class="keywordflow">return</span> 0;
<a name="l00464"></a>00464   }
<a name="l00465"></a>00465   <span class="comment">/* off only if there is no transmission or reception of packet. */</span>
<a name="l00466"></a>00466   <span class="keywordflow">if</span>(onoroff == ON &amp;&amp; TXBUF_EMPTY() &amp;&amp; !receiving_packet){
<a name="l00467"></a>00467     LED_RDC_OFF();
<a name="l00468"></a>00468     ST_RadioSleep();
<a name="l00469"></a>00469     onoroff = OFF;
<a name="l00470"></a>00470     CLEAN_TXBUF();
<a name="l00471"></a>00471     CLEAN_RXBUFS();  
<a name="l00472"></a>00472   
<a name="l00473"></a>00473     ENERGEST_OFF(ENERGEST_TYPE_LISTEN);
<a name="l00474"></a>00474   }
<a name="l00475"></a>00475   
<a name="l00476"></a>00476   <span class="keywordflow">return</span> 1;
<a name="l00477"></a>00477 }
