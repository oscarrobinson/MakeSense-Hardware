<a name="l00674"></a>00674 <span class="keyword">static</span> <span class="keywordtype">int</span> read_from_rxbuf(<span class="keywordtype">void</span> * dest, <span class="keywordtype">unsigned</span> <span class="keywordtype">short</span> len)
<a name="l00675"></a>00675 {
<a name="l00676"></a>00676     
<a name="l00677"></a>00677     <span class="keywordflow">if</span>(RXBUFS_EMPTY()){ <span class="comment">// Buffers are all empty</span>
<a name="l00678"></a>00678         <span class="keywordflow">return</span> 0;
<a name="l00679"></a>00679     }        
<a name="l00680"></a>00680     
<a name="l00681"></a>00681     <span class="keywordflow">if</span>(stm32w_rxbufs[first][0] &gt; len){  <span class="comment">// Too large packet for dest.</span>
<a name="l00682"></a>00682         len = 0;
<a name="l00683"></a>00683     }
<a name="l00684"></a>00684     <span class="keywordflow">else</span> {        
<a name="l00685"></a>00685         len = stm32w_rxbufs[first][0];            
<a name="l00686"></a>00686         memcpy(dest,stm32w_rxbufs[first]+1,len);
<a name="l00687"></a>00687         packetbuf_set_attr(PACKETBUF_ATTR_RSSI, last_rssi);
<a name="l00688"></a>00688     }
<a name="l00689"></a>00689     
<a name="l00690"></a>00690 <span class="preprocessor">#if RADIO_RXBUFS &gt; 1    </span>
<a name="l00691"></a>00691 <span class="preprocessor"></span>    <a class="code" href="a01960.html#gadb86d22878544b15c38b727363ecb8b7" title="A block of code may be made atomic by wrapping it with this macro.">ATOMIC</a>(
<a name="l00692"></a>00692            first = (first + 1) % RADIO_RXBUFS;
<a name="l00693"></a>00693            <span class="keywordtype">int</span> first_tmp = first;
<a name="l00694"></a>00694            <span class="keywordflow">if</span>(first_tmp == last){
<a name="l00695"></a>00695                CLEAN_RXBUFS();
