<a name="l00267"></a>00267 } <a class="code" href="a01929.html#ga5b40bc7412028a269b95ce9caaafe693" title="ND option MTU.">uip_nd6_opt_mtu</a>;
<a name="l00268"></a>00268 <span class="comment"></span>
<a name="l00269"></a>00269 <span class="comment">/** \struct Redirected header option */</span>
<a name="l00270"></a>00270 <span class="keyword">typedef</span> <span class="keyword">struct </span>uip_nd6_opt_redirected_hdr {
<a name="l00271"></a>00271   uint8_t type;
