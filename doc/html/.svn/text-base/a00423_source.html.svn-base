<a name="l00248"></a>00248 <span class="comment"></span>
<a name="l00249"></a>00249 <span class="comment">/** \brief ND option prefix information */</span>
<a name="l00250"></a><a class="code" href="a00117.html">00250</a> <span class="keyword">typedef</span> <span class="keyword">struct </span><a class="code" href="a00117.html" title="ND option prefix information.">uip_nd6_opt_prefix_info</a> {
<a name="l00251"></a>00251   uint8_t type;
<a name="l00252"></a>00252   uint8_t len;
<a name="l00253"></a>00253   uint8_t preflen;
<a name="l00254"></a>00254   uint8_t flagsreserved1;
<a name="l00255"></a>00255   uint32_t validlt;
<a name="l00256"></a>00256   uint32_t preferredlt;
<a name="l00257"></a>00257   uint32_t reserved2;
<a name="l00258"></a>00258   <a class="code" href="a00112.html" title="Representation of an IP address.">uip_ipaddr_t</a> prefix;
<a name="l00259"></a>00259 } <a class="code" href="a01929.html#ga0dd13e3b5fe2b630f5092d4a0e7ba083" title="ND option prefix information.">uip_nd6_opt_prefix_info</a> ;
<a name="l00260"></a>00260 <span class="comment"></span>
<a name="l00261"></a>00261 <span class="comment">/** \brief ND option MTU */</span>
<a name="l00262"></a><a class="code" href="a00116.html">00262</a> <span class="keyword">typedef</span> <span class="keyword">struct </span><a class="code" href="a00116.html" title="ND option MTU.">uip_nd6_opt_mtu</a> {
<a name="l00263"></a>00263   uint8_t type;
