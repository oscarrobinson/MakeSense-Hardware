<a name="l00117"></a>00117   <span class="keywordtype">int</span> ret;
<a name="l00118"></a>00118   <span class="keywordflow">if</span>(outputfunc != <a class="code" href="a01956.html#ga070d2ce7b6bb7e5c05602aa8c308d0c4" title="The null pointer.">NULL</a>) {
<a name="l00119"></a>00119     ret = outputfunc(a);
<a name="l00120"></a>00120     <span class="keywordflow">return</span> ret;
<a name="l00121"></a>00121   }
<a name="l00122"></a>00122   UIP_LOG(<span class="stringliteral">&quot;tcpip_output: Use tcpip_set_outputfunc() to set an output function&quot;</span>);
<a name="l00123"></a>00123   <span class="keywordflow">return</span> 0;
<a name="l00124"></a>00124 }
<a name="l00125"></a>00125 
